[opencast]
name = Opencast 10 el $releasever Repository
baseurl  = https://pkg.opencast.org/rpms/release/el/$releasever/oc-10/x86_64
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-10-rpm.key

[opencast-noarch]
name = Opencast 10 el $releasever Repository - noarch
baseurl  = https://pkg.opencast.org/rpms/release/el/$releasever/oc-10/noarch
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-10-rpm.key

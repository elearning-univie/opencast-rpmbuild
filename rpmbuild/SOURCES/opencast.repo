[opencast]
name = Opencast OC_VERSION el $releasever Repository
baseurl  = https://pkg.opencast.org/rpms/release/el/$releasever/oc-OC_VERSION/x86_64
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-OC_VERSION-rpm.key

[opencast-noarch]
name = Opencast OC_VERSION el $releasever Repository - noarch
baseurl  = https://pkg.opencast.org/rpms/release/el/$releasever/oc-OC_VERSION/noarch
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-OC_VERSION-rpm.key

[opencast-nightly]
name = Opencast 14 el$releasever nightly builds
baseurl  = https://pkg.opencast.org/opencast-rpm/nightly/el$releasever/oc-14/x86_64
enabled  = 0
gpgcheck = 1
gpgkey = https://pkg.opencast.org/opencast-rpm/gpgkeys/opencast-14-rpm.key

[opencast-nightly-noarch]
name = Opencast 14 el$releasever nightly noarch builds
baseurl  = https://pkg.opencast.org/opencast-rpm/nightly/el$releasever/oc-14/noarch
enabled  = 0
gpgcheck = 1
gpgkey = https://pkg.opencast.org/opencast-rpm/gpgkeys/opencast-14-rpm.key
